 *******************************************/

#ifndef <%s.NAME%>_STRUCT_PRIVATE_H
#define <%s.NAME%>_STRUCT_PRIVATE_H

#include "<%s.name.toLowerCase()%>-struct.h"

namespace <%model.name%> {

class <%s.Name%>Private {
public:
<%
    var str = '';
    s.members.forEach( function(m) 
    {
        var substr =
'    ${memberType} ${member};\n';

        substr = substr.replace(/\${memberType}/g, m.type.name)
                       .replace(/\${member}/g, m.name);
        str += substr;
    } );
    str;
%>
};

}

#endif // <%s.NAME%>_STRUCT_PRIVATE_H
